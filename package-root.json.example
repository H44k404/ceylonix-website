{
  "name": "ceylonix-website",
  "version": "1.0.0",
  "description": "Ceylonix Photography Website - React Frontend + Node.js Backend",
  "private": true,
  "scripts": {
    "start": "echo Please run 'npm start' in the frontend directory or 'npm start' in ceylonix-backend directory",
    "frontend": "npm start --prefix ./",
    "backend": "npm start --prefix ./ceylonix-backend",
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "build": "npm run build --prefix ./",
    "install-all": "npm install && npm install --prefix ./ceylonix-backend",
    "setup": "npm run install-all"
  },
  "keywords": [
    "photography",
    "portfolio",
    "react",
    "nodejs",
    "express"
  ],
  "author": "Ceylonix.CMB",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/ceylonix-website.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/ceylonix-website/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/ceylonix-website#readme"
}
